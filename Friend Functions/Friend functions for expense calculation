void expenses(HotelBill h1,Bus t)
{
    double e;
    e=h1.totalBill+t.cost;
    cout<<"Your total expense is: "<<e<<endl;
    return;
}
void expenses(HotelBill h1,Aeroplane t)
{
    double e;
    e=h1.totalBill+t.cost;
    cout<<"Your total expense is: "<<e<<endl;
    return;
}
void expenses(HotelBill h1,Train t)
{
    double e;
    e=h1.totalBill+t.cost;
    cout<<"Your total expense is: "<<e<<endl;
    return;
}
